<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    
    <!-- Spring Boot関連の誤検知を抑制 -->
    <suppress>
        <notes>Spring Boot自体のセキュリティは適切に管理されているため</notes>
        <packageUrl regex="true">^pkg:maven/org\.springframework\.boot/.*$</packageUrl>
        <cve>CVE-2016-1000027</cve>
    </suppress>

    <!-- MyBatis関連の誤検知を抑制 -->
    <suppress>
        <notes>MyBatisの古いバージョンに関する誤検知</notes>
        <packageUrl regex="true">^pkg:maven/org\.mybatis/.*$</packageUrl>
        <vulnerabilityName>mybatis-3.4.1.jar</vulnerabilityName>
    </suppress>

    <!-- 開発環境専用の依存関係（テスト用など）を抑制 -->
    <suppress>
        <notes>テスト環境でのみ使用される依存関係</notes>
        <packageUrl regex="true">^pkg:maven/.*test.*$</packageUrl>
        <cvssBelow>8</cvssBelow>
    </suppress>

</suppressions>